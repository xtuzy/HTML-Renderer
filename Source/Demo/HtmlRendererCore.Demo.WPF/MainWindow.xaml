<Window
    x:Class="HtmlRendererCore.Demo.WPF.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:HtmlRendererCore.Demo.WPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wpf="clr-namespace:TheArtOfDev.HtmlRenderer.WPF;assembly=HTMLRendererCore.WPF"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <ScrollViewer>
        <StackPanel>
            <wpf:HtmlLabel x:Name="htmlLabel" />
            <wpf:HtmlPanel x:Name="htmlPanel" />
        </StackPanel>
    </ScrollViewer>
</Window>
